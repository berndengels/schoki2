{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "anhskohbo/no-captcha": "^3.3",
        "bepsvpt/secure-headers": "^7.0",
        "berndengels/laravel8-email-exceptions": "^1.0",
        "brackets/craftable": "^6.0",
        "bumbummen99/shoppingcart": "^3.0",
        "eluceo/ical": "^0.16.1",
        "fideloper/proxy": "^4.4",
        "friendsofphp/php-cs-fixer": "^2.17",
        "fruitcake/laravel-cors": "^2.0",
        "garygreen/pretty-routes": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ixudra/curl": "^6.21",
        "kalnoy/nestedset": "^5.0",
        "kris/laravel-form-builder": "^1.40",
        "laravel/cashier": "^12.6",
        "laravel/framework": "8.16",
        "laravel/telescope": "^4.3",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "laravelium/feed": "^8.0",
        "lavary/laravel-menu": "^1.7",
        "misterphilip/maintenance-mode": "^2.0",
        "phpstan/phpstan": "^0.12.62",
        "spatie/laravel-db-snapshots": "^1.7",
        "spatie/laravel-newsletter": "^4.9",
        "spatie/laravel-stripe-webhooks": "^2.4",
        "srmklive/paypal": "^1.0",
        "swiftmailer/swiftmailer": "^6.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "brackets/admin-generator": "^6.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan telescope:publish --ansi"
        ]
    }
}
